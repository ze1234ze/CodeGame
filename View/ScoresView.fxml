<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" stylesheets="@/CodeGame/CSS/MyStyle.css" fx:controller="CodeGame.Controller.ScoresViewController">
   <children>
      <TableView fx:id="scoreTable" layoutX="217.0" layoutY="245.0" prefHeight="323.0" prefWidth="354.0">
        <columns>
          <TableColumn fx:id="playername" prefWidth="219.0" text="Player name" />
          <TableColumn fx:id="score" prefWidth="134.0" text="Score" />
        </columns>
      </TableView>
      <Label layoutX="246.0" layoutY="121.0" prefHeight="111.0" prefWidth="309.0" text="Scores">
         <font>
            <Font name="Castellar" size="76.0" />
         </font>
      </Label>
      <Button fx:id="resetScore" layoutX="420.0" layoutY="605.0" mnemonicParsing="false" onAction="#resetScoreOnClick" text="Reset scores">
         <font>
            <Font name="Castellar" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
